#include <graphics.h>
#include <conio.h>

int main() {
    int gd = DETECT, gm;
    initgraph(&gd, &gm, "");

    setcolor(RED);  // Set the color to red
    circle(300, 200, 50);  // Draw a red circle at (300, 200) with radius 50

    setcolor(GREEN);  // Set the color to green
    rectangle(100, 100, 300, 200);  // Draw a green rectangle

    getch();
    closegraph();
    return 0;
}
